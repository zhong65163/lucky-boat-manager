{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/app.js",
      "destination": "/web/app.js"
    },
    {
      "source": "/web/(.*)",
      "destination": "/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}